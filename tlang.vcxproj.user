<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ShowAllFiles>false</ShowAllFiles>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <LocalDebuggerCommandArguments>--keep-temp --profile a.tl</LocalDebuggerCommandArguments>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerWorkingDirectory>$(ProjectDir)devtest</LocalDebuggerWorkingDirectory>
    <RemoteDebuggerCommandArguments>--keep-temp --profile a.tl</RemoteDebuggerCommandArguments>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <LocalDebuggerCommandArguments>--keep-temp --profile a.tl</LocalDebuggerCommandArguments>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerWorkingDirectory>$(ProjectDir)devtest</LocalDebuggerWorkingDirectory>
    <RemoteDebuggerCommandArguments>--keep-temp --profile a.tl</RemoteDebuggerCommandArguments>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='DebugFast|x64'">
    <LocalDebuggerCommandArguments>--stats --target nasm_x86_64_windows --keep-temp --profile performance_test.tl</LocalDebuggerCommandArguments>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerWorkingDirectory>$(ProjectDir)devtest</LocalDebuggerWorkingDirectory>
    <RemoteDebuggerCommandArguments>--stats --target nasm_x86_64_windows --keep-temp --profile performance_test.tl</RemoteDebuggerCommandArguments>
  </PropertyGroup>
</Project>